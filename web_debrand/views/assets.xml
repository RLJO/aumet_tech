<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="inherit_digest_assets_backend" name="Backend Assets" inherit_id="web.assets_backend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/web_debrand/static/src/css/style.css"/>
        </xpath>
    </template>
    <template id="inherit_digest_section_mobile"  inherit_id="digest.digest_section_mobile">
        <xpath expr="//div" position="replace">
            <div></div>
        </xpath>
    </template>

    <template id="inherit_digest_mail_main"  inherit_id="digest.digest_mail_main">
        <xpath expr="//div[hasclass('global_layout')]" position="replace">
            <div></div>
        </xpath>
    </template>
<template id="assets" inherit_id="point_of_sale.assets">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/web_debrand/static/src/css/style.css"/>
        </xpath>
    </template>
</odoo>