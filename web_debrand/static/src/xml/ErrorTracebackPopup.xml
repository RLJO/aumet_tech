<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">


         <t t-name="ErrorTracebackPopup" t-inherit="point_of_sale.ErrorTracebackPopup" t-inherit-mode="extension"
            owl="1">
        <xpath expr="//main[hasclass('body')]" position="replace">
            <p class="body">An issue has occurred, please contact our support team</p>
          </xpath>
             <xpath expr="//a[@t-att-download='tracebackFilename']" position="replace"/>
             <xpath expr="//footer//div[hasclass('email')]" position="replace"/>

    </t>

</templates>
